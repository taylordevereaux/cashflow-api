{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/CashFlow.Api/CashFlow.Api.csproj"],
      "problemMatcher": "$tsc"
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/CashFlow.Api/CashFlow.Api.csproj"
      ],
      "problemMatcher": "$tsc"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": ["publish", "${workspaceFolder}/CashFlow.Api.csproj"],
      "problemMatcher": "$tsc"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": ["watch", "run", "${workspaceFolder}/CashFlow.Api.csproj"],
      "problemMatcher": "$tsc"
    }
  ]
}
